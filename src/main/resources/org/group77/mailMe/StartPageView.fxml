<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.group77.mailMe.controller.StartPageController">
    <children>
        <BorderPane fx:id="startPageBorderPane" prefHeight="400.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <Label fx:id="welcomeLabel" text="Choose account or login to a new one!" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets top="40.0" />
                    </BorderPane.margin>
                </Label>
            </top>
         <bottom>
            <Button fx:id="addAccountButton" mnemonicParsing="false" text="Add new account" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" top="20.0" />
               </BorderPane.margin></Button>
         </bottom>
        </BorderPane>
    </children>
</AnchorPane>
